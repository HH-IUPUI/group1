{
  "name": "group1",
  "version": "1.0.0",
  "description": "This directory contains starter files (templates) for a home page.",
  "main": "index.html",
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "concat": "^1.0.3",
    "node-sass": "5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1"
  },
  "devDependencies": {},
  "scripts": {
    "concat:css": "concat -o styles/overallStyles.css styles/countryStyles.css styles/aboutStyles.css styles/indexStyles.css styles/infoStyles.css",
    "prefix:css": "postcss --use autoprefixer -b 'last versions' styles/overallStyles.css -o styles/overallStyles.prefix.css",
    "compress:css": "node-sass styles/overallStyles.prefix.css styles/overallStyles.min.css --output-style compressed",
    "build:css": "npm-run-all concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HH-IUPUI/group1.git"
  },
  "author": "Shannon Schrauwen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HH-IUPUI/group1/issues"
  },
  "homepage": "https://github.com/HH-IUPUI/group1#readme"
}
